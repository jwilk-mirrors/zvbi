msgstr "%s (%s) no soporta la función select()."

#: src/io-v4l2.c:370
#, c-format
msgid "Cannot query current videostandard of %s (%s): %d, %s."
msgstr "No se pueden obtener el estándar actual de %s (%s): %d, %s."

#: src/io-v4l2.c:372 src/io-v4l2.c:644
msgid "Probably a driver bug."
msgstr "Probablemente un fallo del controlador."

#: src/io-v4l2.c:392
#, c-format
msgid "Cannot query current vbi parameters of %s (%s): %d, %s."
msgstr ""
"No se pueden obtener los parámetros de vbi actuales de %s (%s): %d, %s."

#: src/io-v4l2.c:409 src/io-v4l2.c:522 src/io-v4l.c:414 src/io-v4l.c:716
#: src/io-v4l.c:774
#, c-format
msgid "Sorry, %s (%s) cannot capture any of the requested data services."
msgstr ""
"Lo siento, %s (%s) no puede capturar ninguno de los servicios de datos "
"solicitados."

#: src/io-v4l2.c:446 src/io-v4l.c:451
#, c-format
msgid "Cannot initialize %s (%s), the device is already in use."
msgstr "No se puede inicializar %s (%s), ya está siendo utilizado."

#: src/io-v4l2.c:462 src/io-v4l.c:457
#, c-format
msgid "Could not set the vbi capture parameters for %s (%s): %d, %s."
msgstr ""
"No se pudieron establecer los parámetros de captura para %s (%s): %d, %s."

#: src/io-v4l2.c:465 src/io-v4l2.c:564
msgid "Possibly a driver bug."
msgstr "Posiblemente un fallo del driver."

#: src/io-v4l2.c:494 src/io-v4l.c:596
#, c-format
msgid ""
"%s (%s) offers unknown vbi sampling format #%d. This may be a driver bug or "
"libzvbi is too old."
msgstr ""
"%s (%s) ofrece un formato de muestreo no reconocido #%d. Esto puede ser un "
"fallo del controlador o que libzvbi es demasiado vieja."

#: src/io-v4l2.c:506 src/io-v4l.c:760
#, c-format
msgid ""
"Cannot capture the requested data services with %s (%s), the sampling "
"frequency %.2f MHz is too low."
msgstr ""
"No se pueden capturar los servicios de datos solicitados con %s (%s), la "
"frecuencia de muestreo %.2f MHz es demasiado baja."

#: src/io-v4l2.c:561
#, c-format
msgid "Cannot request streaming i/o buffers from %s (%s): %d, %s."
msgstr "No se pueden pedir buffers de streaming de %s (%s): %d, %s."

#: src/io-v4l2.c:569
#, c-format
msgid ""
"%s (%s) granted no streaming i/o buffers, perhaps the physical memory is "
"exhausted."
msgstr ""
"%s (%s) no creó buffers streaming, posiblemente memoria física agotada."

#: src/io-v4l2.c:594
#, c-format
msgid "Querying streaming i/o buffer #%d from %s (%s) failed: %d, %s."
msgstr "Fallo al pedir buffer streaming #%d de %s (%s): %d, %s."

#: src/io-v4l2.c:616
#, c-format
msgid "Memory mapping streaming i/o buffer #%d from %s (%s) failed: %d, %s."
msgstr "Fallo al mapear a memoria el buffer streaming #%d de %s (%s): %d, %s."

#: src/io-v4l2.c:640
#, c-format
msgid "Cannot enqueue streaming i/o buffer #%d to %s (%s): %d, %s."
msgstr "Fallo al enviar el buffer streaming #%d a %s (%s): %d, %s."

#: src/io-v4l2.c:672 src/io-v4l.c:815
#, c-format
msgid "Not enough memory to allocate vbi capture buffer (%d KB)."
msgstr "No hay sufficiente memoria para el buffer de captura VBI (%d KB)."

#: src/io-v4l2.c:682
#, c-format
msgid ""
"%s (%s) lacks a vbi read interface, possibly an output only device or a "
"driver bug."
msgstr ""
"%s (%s) no tiene un interfaz de lectura, posiblemente un dispositivo\n"
"de sólo salida o un fallo del controlador."

#: src/io-v4l2.c:733
msgid "V4L2 interface not compiled."
msgstr "Interfaz V4L2 no compilado."

#: src/io-v4l.c:508
msgid "driver unknown"
msgstr "controlador desconocido"

#: src/io-v4l.c:639
#, c-format
msgid "Cannot capture with %s (%s), has no standard vbi interface."
msgstr "No se puede capturar con %s (%s), no tiene interfaz vbi estándar."

#: src/io-v4l.c:657
#, c-format
msgid ""
"Cannot identify %s (%s), reported vbi frame size suggests this is not a bttv "
"driver."
msgstr ""
"No se puede identificar %s (%s), el tamaño de cuadro vbi sugiere que no es "
"un controlador bttv."

#: src/io-v4l.c:673 src/io-v4l.c:733
#, c-format
msgid "Cannot set or determine current videostandard of %s (%s)."
msgstr "No se puede establecer o determinar el estándar actual de %s (%s)."

#: src/io-v4l.c:863 src/io-v4l.c:873
msgid "V4L interface not compiled."
msgstr "Interfaz V4L no compilado."
